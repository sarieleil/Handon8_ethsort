#include <iostream>

#define MAX_SIZE 100  

class Stack {
private:
    int A[MAX_SIZE];
    int top;
public:
    Stack() : top(-1) {}

    void push(int x) {
        if (top == MAX_SIZE - 1) {
            std::cerr << "Stack overflow\n";
            return;
        }
        A[++top] = x;
    }

    void pop() {
        if (top == -1) {
            std::cerr << "Stack underflow\n";
            return;
        }
        top--;
    }

    int topElement() {
        if (top == -1) {
            std::cerr << "Stack is empty\n";
            return -1;
        }
        return A[top];
    }

    bool isEmpty() {
        return top == -1;
    }
};

int main() {
    Stack s;
    s.push(10);
    s.push(20);
    std::cout << "Top element: " << s.topElement() << std::endl;
    s.pop();
    std::cout << "Top element after pop: " << s.topElement() << std::endl;
    return 0;
}
